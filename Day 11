# Two sum solution
class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        num_map=dict()
        for i in range(len(nums)):
            complement=target-nums[i]
            if (complement in num_map):
                return [num_map[complement],i]
            else:
                num_map[nums[i]]=i
                print(num_map)
